{
  "entryFile": "apps/express-api/src/app.ts",
  "noImplicitAdditionalProperties": "silently-remove-extras",
  "controllerPathGlobs": ["apps/express-api/src/controllers/*Controller.ts"],
  "spec": {
    "outputDirectory": "apps/express-api/generated/spec",
    "specVersion": 3
  },
  "routes": {
    "routesDir": "apps/express-api/generated/routes"
  },
  "compilerOptions": {
    "paths": {
      "@nx-monorepo-templates/models": ["libs/models/src/index.ts"]
    }
  }
}
