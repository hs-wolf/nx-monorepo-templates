{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES2020", "DOM"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": ".",
    "module": "ESNext",
    "paths": {
      "@nx-monorepo-templates/models": ["libs/models/src/index.ts"]
    },
    "resolveJsonModule": true,
    "rootDir": ".",
    "moduleResolution": "Node10",
    "declaration": false,
    "importHelpers": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "tmp"]
}
