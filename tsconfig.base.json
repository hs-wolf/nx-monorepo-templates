{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "ES2020"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": ".",
    "module": "ESNext",
    "paths": {},
    "rootDir": ".",
    "moduleResolution": "Node10",
    "declaration": false,
    "importHelpers": true,
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}